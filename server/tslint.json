{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "object-literal-sort-keys": [
      false,
      "dev"
    ],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}
