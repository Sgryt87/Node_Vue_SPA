{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node Debug",
      "args": ["src/index.ts"],
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${workspaceFolder}/server",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart",
      "env": { "NODE_ENV": "development" },
      "restart": true
    }
  ],
  "compounds": []
}
